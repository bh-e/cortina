#!/usr/bin/make -f
# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

%:
	dh $@ 
override_dh_build: 
	dh_testdir
	dh_auto_configure

override_dh_auto_configure:
	qmake-qt4 -makefile -nocache "QMAKE_CFLAGS_RELEASE=$(shell dpkg-buildflags --get CFLAGS)"  "QMAKE_CXXFLAGS_RELEASE=$(shell dpkg-buildflags --get CXXFLAGS)" "QMAKE_LFLAGS_RELEASE=$(shell dpkg-buildflags --get LDFLAGS)" 

override_dh_auto_build: 
	lrelease Cortina.pro -silent
	$(make)
